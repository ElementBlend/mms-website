{
  "/api/v1/auth/login": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug"
  },
  "/api/v1/auth/token": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug"
  },
  "/api/v1/modpacks/timelines": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug"
  },
  "/api/v1/modpacks/versions": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug"
  },
  "/api/v1/modpacks": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/v1/modpacks/(.*)": "/api/v1/modpacks/$1"
    }
  },
  "/api/v1/app/cert-installer": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/v1/app/cert-installer/(.*)": "/api/v1/app/cert-installer/$1"
    }
  },
  "/api/v1/app/cert-installer/hash": {
    "target": "https://localhost:3000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/v1/app/cert-installer/hash/(.*)": "/api/v1/app/cert-installer/hash/$1"
    }
  }
}
